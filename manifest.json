{
  "name": "Deutsche Tastatur mit Shortcuts",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Deutsche Tastatur mit Shortcuts",
  "background": {
    "scripts": [
      "main.js"
    ]
  },
  "permissions": [
    "input"
  ],
  "commands": {
    "00-up": {
      "description": "Go Up"
    },
    "01-down": {
      "description": "Go Down"
    },
    "02-left": {
      "description": "Go Left",
      "suggested_key": {
        "default": "Ctrl+B"
      }
    },
    "03-right": {
      "description": "Go Right"
    },
    "10-home": {
      "description": "Go Home"
    },
    "11-end": {
      "description": "Go End",
      "suggested_key": {
        "default": "Ctrl+E"
      }
    },
    "20-delete": {
      "description": "Delete"
    },
    "30-backspace": {
      "description": "Backspace"
    },
    "40-enter": {
      "description": "Enter",
      "suggested_key": {
        "default": "Ctrl+M"
      }
    },
    "50-killend": {
      "description": "Kill line"
    },
    "60-cut": {
      "description": "Cut"
    },
    "61-paste": {
      "description": "Paste",
      "suggested_key": {
        "default": "Ctrl+Y"
      }
    },
    "90-original-ctrl-f": {
      "description": "Original Ctrl+F"
    }
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "input_components": [
    {
      "name": "Deutsche Tastatur mit Shortcuts",
      "type": "ime",
      "id": "Deutsche_Tastatur_mit_Shortcuts",
      "description": "Deutsche Tastatur mit Shortcuts",
      "language": "de",
      "layouts": ["de"]
    }
  ]
}
